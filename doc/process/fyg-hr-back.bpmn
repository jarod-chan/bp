  <message id="triggerBack" name="triggerBackMessage"></message>
  <process id="fyg-hr-back" name="销假流程">
    <startEvent id="messageStart" name="messageStart">
      <messageEventDefinition  messageRef="triggerBack"></messageEventDefinition >
    </startEvent>
    <userTask id="dm_check" name="部门经理确认" activiti:assignee="chenzw" activiti:formKey="hr/back/check_dm"></userTask>
    <userTask id="vp_check" name="分管副总确认" activiti:assignee="chenzw" activiti:formKey="hr/back/check_vp"></userTask>
    <userTask id="gm_check" name="总经理确认" activiti:assignee="chenzw" activiti:formKey="hr/back/check_gm"></userTask>
    <startEvent id="start" name="start" activiti:formKey="hr/back/start"></startEvent>
    <serviceTask id="produceBack" name="自动生成销假" activiti:class="cn.fyg.bp.interfaces.web.module.hr.back.flow.ProduceBack">
      <extensionElements>
        <activiti:field name="leaveServiceExp">
          <activiti:expression>${leaveService}</activiti:expression>
        </activiti:field>
        <activiti:field name="backServiceExp">
          <activiti:expression>${backService}</activiti:expression>
        </activiti:field>
        <activiti:field name="compdateServiceExp">
          <activiti:expression>${compdateService}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <userTask id="staff_modify" name="员工修改销假" activiti:assignee="chenzw" activiti:formKey="hr/back/edit"></userTask>
    <exclusiveGateway id="dm_pass" name="部门经理确认通过"></exclusiveGateway>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <exclusiveGateway id="normalBack" name="是否正常销假"></exclusiveGateway>
    <endEvent id="endevent1" name="End"></endEvent>
    <serviceTask id="staff_notigy" name="邮件通知" activiti:type="mail">
      <extensionElements>
        <activiti:field name="to" expression="232879583@qq.com"></activiti:field>
        <activiti:field name="subject" expression="发送邮件测试"></activiti:field>
        <activiti:field name="charset" expression="utf-8"></activiti:field>
        <activiti:field name="html">
          <activiti:expression><![CDATA[<html>
<body>
<html>
<body>
              你好：<br/><br/>
                 
              &nbsp;&nbsp;编号：<b>${businessNo}</b>销假流程已经结束。<br/><br/>
                  
		  <br/>
</body>
</html>
</body>
</html>]]></activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="exclusivegateway4" name="Exclusive Gateway"></exclusiveGateway>
    <exclusiveGateway id="in3day" name="是否3天内"></exclusiveGateway>
    <serviceTask id="set_begin" name="审批开始" activiti:class="cn.fyg.bp.interfaces.web.module.hr.back.flow.BeginSet">
      <extensionElements>
        <activiti:field name="backServiceExp">
          <activiti:expression>${backService}</activiti:expression>
        </activiti:field>
        <activiti:field name="opinionServiceExp">
          <activiti:expression>${opinionService}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <serviceTask id="set_end" name="审批结束" activiti:class="cn.fyg.bp.interfaces.web.module.hr.back.flow.EndSet">
      <extensionElements>
        <activiti:field name="backServiceExp">
          <activiti:expression>${backService}</activiti:expression>
        </activiti:field>
      </extensionElements>
    </serviceTask>
    <exclusiveGateway id="exclusivegateway5" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow1" name="系统启动" sourceRef="messageStart" targetRef="produceBack"></sequenceFlow>
    <sequenceFlow id="flow2" name="" sourceRef="produceBack" targetRef="staff_modify"></sequenceFlow>
    <sequenceFlow id="flow3" name="手动启动" sourceRef="start" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow8" name="" sourceRef="vp_check" targetRef="exclusivegateway5"></sequenceFlow>
    <sequenceFlow id="flow9" name="" sourceRef="staff_modify" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow10" name="" sourceRef="exclusivegateway2" targetRef="set_begin"></sequenceFlow>
    <sequenceFlow id="flow12" name="确认未通过" sourceRef="dm_pass" targetRef="staff_modify">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isPass}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" name="确认" sourceRef="dm_pass" targetRef="normalBack">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isPass}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow14" name="正常销假" sourceRef="normalBack" targetRef="exclusivegateway4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isNormalBack}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow15" name="" sourceRef="staff_notigy" targetRef="endevent1"></sequenceFlow>
    <sequenceFlow id="flow16" name="" sourceRef="exclusivegateway4" targetRef="set_end"></sequenceFlow>
    <sequenceFlow id="flow17" name="其它" sourceRef="normalBack" targetRef="in3day">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isNormalBack}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow18" name="小于3天" sourceRef="in3day" targetRef="exclusivegateway4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${acturlDay<=3}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow19" name="大于3天" sourceRef="in3day" targetRef="vp_check">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${acturlDay>3}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow20" name="大于7天" sourceRef="exclusivegateway5" targetRef="gm_check">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${acturlDay>7}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow21" name="" sourceRef="gm_check" targetRef="exclusivegateway4"></sequenceFlow>
    <sequenceFlow id="flow22" name="小于7天" sourceRef="exclusivegateway5" targetRef="exclusivegateway4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${acturlDay<=7}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow23" name="" sourceRef="dm_check" targetRef="dm_pass"></sequenceFlow>
    <sequenceFlow id="flow25" name="" sourceRef="set_end" targetRef="staff_notigy"></sequenceFlow>
    <sequenceFlow id="flow26" name="" sourceRef="set_begin" targetRef="dm_check"></sequenceFlow>
